{
  "version": 2,
  "builds": [
    {
      "src": "bzmarket/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "maxLambdaSize": "15mb",
        "buildCommand": "bash build.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)$",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "^(.*)$",
      "dest": "bzmarket/wsgi.py"
    }
  ]
}
